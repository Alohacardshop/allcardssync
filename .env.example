# Legacy catalog variables (DEPRECATED - functionality moved to external service):
# VITE_USE_CATALOG_V2_MTG="true"
# VITE_USE_CATALOG_V2_POKEMON="true"
# VITE_USE_CATALOG_V2_POKEMON_JAPAN="true"

# Supabase Configuration (auto-configured in production)
# Get these from your Supabase project settings: https://supabase.com/dashboard/project/settings/api
VITE_SUPABASE_URL=your-supabase-project-url-here
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# API Keys (set in Supabase Secrets)  
# Get Pokemon TCG API key from: https://dev.pokemontcg.io/
POKEMONTCG_API_KEY=your-pokemon-tcg-api-key-here
# Get JustTCG API key from: https://docs.justtcg.com/
JUSTTCG_API_KEY=your-justtcg-api-key-here

# TCG Database Configuration (External read-only database)
# Contact your TCG database provider for these credentials
VITE_TCG_URL=your-tcg-database-url-here
VITE_TCG_ANON_KEY=your-tcg-database-anon-key-here

# External TCG Database (backup/alternative source)  
# Contact your backup TCG database provider for these credentials
VITE_EXTERNAL_TCG_URL=your-external-tcg-database-url-here
VITE_EXTERNAL_TCG_ANON_KEY=your-external-tcg-database-anon-key-here

# Print Bridge Configuration (Local network settings)
VITE_LOCAL_BRIDGE_URL=http://localhost:3001
VITE_DEFAULT_PRINTER_IP=192.168.1.70
VITE_DEFAULT_PRINTER_PORT=9100

# Shopify Configuration (stored in database/Supabase secrets)
# These are managed through the admin interface, not environment variables

# Production Deployment Settings
NODE_ENV=production
VITE_APP_VERSION=1.0.0